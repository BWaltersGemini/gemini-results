{
  "rewrites": [
    {
      "source": "/chrono-api/(.*)",
      "destination": "https://api.chronotrack.com/$1"
    },
    {
      "source": "/:path*/assets/(.*)",
      "destination": "/assets/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/:path*/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}